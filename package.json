{
  "name": "babel-plugin-import-auto-name",
  "version": "0.1.0",
  "description": "Babel plugin to auto-expand nameless imports",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -fr lib && babel -s inline -d lib/ src/",
    "test": "ava --verbose"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shnhrrsn/babel-plugin-import-auto-name.git"
  },
  "keywords": [
    "babel",
    "babel-plugin",
    "module",
    "require",
    "import"
  ],
  "author": {
    "name": "Shaun Harrison",
    "email": "npm@shaun.harrison.xyz",
    "url": "http://twitter.com/shnhrrsn"
  },
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.18.0",
    "babel-cli": "^6.22.2",
    "babel-preset-es2016-node5": "^1.1.2",
    "babel-register": "^6.22.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
